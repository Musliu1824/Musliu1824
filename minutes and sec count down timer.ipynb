    "import time\n",
    "\n",
    "def countdown(tm):\n",
    "   \n",
    "    while tm:\n",
    "        m,s = divmod(tm, 60)\n",
    "        print('{:2d}m:{:2d}s'.format(m,s))\n",
    "        time.sleep(1)\n",
    "        tm -=1 \n",
    "        \n",
    "countdown(tm = int(input('Enter count down time : ')))\n",
    "print('time elapsed')   \n",
    "    \n"
   ]
