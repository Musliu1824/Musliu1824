   "source": [
    "n_factorial = int(input('enter number : '))\n",
    "p=1\n",
    "\n",
    "for i in range(1,n_factorial+1):\n",
    "    if n_factorial < 0:\n",
    "        print(' no factorial of a negative number ')\n",
    "    elif n_factorial==0:\n",
    "        print(\" factorial of 0 is 1 \")\n",
    "    else:\n",
    "        p = p*i\n",
    "print(p)"
